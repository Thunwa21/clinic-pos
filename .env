# Clinic POS - Environment Variables
# Copy to .env and adjust values as needed

# PostgreSQL
POSTGRES_USER=clinic_user
POSTGRES_PASSWORD=clinic_pass
POSTGRES_DB=clinic_pos

# Redis
REDIS_PASSWORD=clinic_redis_pass

# RabbitMQ
RABBITMQ_DEFAULT_USER=guest
RABBITMQ_DEFAULT_PASS=guest

# Backend
CONNECTION_STRING=Host=postgres;Port=5432;Database=clinic_pos;Username=clinic_user;Password=clinic_pass
REDIS_CONNECTION=redis:6379,password=clinic_redis_pass
RABBITMQ_HOST=rabbitmq
RABBITMQ_USER=guest
RABBITMQ_PASS=guest

# Frontend
NEXT_PUBLIC_API_URL=http://localhost:5001
